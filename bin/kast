#!/usr/bin/env sh

#LD_LIBRARY_PATH="$(dirname $0)/../lib/native/linux/x64" java -Xcheck:jni -ea -ss8m -Xms64m -Xmx1G -jar "$(dirname $0)/../target/k-3.0.jar" -kast "$@"
if [ -z "$K_OPTS" ];
  then export K_OPTS="-Xms64m -Xmx1024m -Xss32m"
fi
if "$(dirname $0)/../src/main/scripts/checkJava"; then
  java -Djava.awt.headless=true $K_OPTS -ea -jar "$(dirname $0)/../target/k-3.0.jar" -kast "$@"
else
  exit 1
fi
