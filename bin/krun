#!/usr/bin/env sh
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:"$(dirname $0)/../target/dependency"
ulimit -s `ulimit -H -s`
if [ -z "$K_OPTS" ];
  then export K_OPTS="-Xms64m -Xmx1024m -Xss32m"
fi
if "$(dirname $0)/../src/main/scripts/checkJava"; then
  java -Djava.awt.headless=true $K_OPTS -ea -jar "$(dirname $0)/../target/k-3.0.jar" -krun "$@"
else
  exit 1
fi
